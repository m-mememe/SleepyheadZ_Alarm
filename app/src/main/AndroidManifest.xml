<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.niked4.wings.android.sleepyheadzAlarm">

    <application
        android:name="com.niked4.wings.android.sleepyheadzAlarm.CustomApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <receiver android:name="com.niked4.wings.android.sleepyheadzAlarm.AlarmBroadcastReceiver"
            android:process=":remote"/>
        <service
            android:name="com.niked4.wings.android.sleepyheadzAlarm.PlayMusicService"
            android:enabled="true"
            android:exported="false"></service>

        <activity android:name="com.niked4.wings.android.sleepyheadzAlarm.TimerMenuActivity" />
        <activity android:name="com.niked4.wings.android.sleepyheadzAlarm.PlayMusicActivity" />
        <activity android:name="com.niked4.wings.android.sleepyheadzAlarm.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>